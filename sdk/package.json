{
  "name": "@eigen-secret/sdk",
  "version": "1.0.7",
  "description": "> TODO: description",
  "author": "Eigen Labs",
  "homepage": "https://github.com/0xEigenLabs/eigen-secret#readme",
  "license": "UNLICENSED",
  "main": "dist-node/index.js",
  "exports": {
    ".": {
      "require": "./dist-node/index.js",
      "import": "./dist-node/index.js",
      "node": "./dist-node/index.js"
    },
    "./dist-node/*": {
      "require": "./dist-node/*.js",
      "import": "./dist-node/*.js",
      "node": "./dist-node/*.js"
    },
    "./dist-browser/*": {
      "require": "./dist-browser/*.js",
      "import": "./dist-browser/*.js"
    }
  },
  "directories": {
    "lib": "lib",
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xEigenLabs/eigen-secret.git"
  },
  "scripts": {
    "build": "npm run build:node && npm run build:browser",
    "build:node": "npx tsc --project tsconfig.node.json",
    "build:browser": "npx tsc --project tsconfig.browser.json",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/0xEigenLabs/eigen-secret/issues"
  },
  "dependencies": {
    "@eigen-secret/core": "file:../core",
    "axios": "^1.3.4",
    "blake-hash": "^2.0.0",
    "circomlibjs": "^0.1.7"
  },
  "devDependencies": {
    "@eslint/create-config": "^0.1.2",
    "@types/chai": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "circom_runtime": "^0.1.18",
    "circom_tester": "^0.0.19",
    "circomlib": "^2.0.5",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.2"
  }
}
