{
  "name": "eigen-secret",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "lerna": "^3.22.1"
  },
  "scripts": {
    "server": "npm run build:server && npm start",
    "compile": "cd secret && hardhat compile",
    "build:core": "cd core && npm run build",
    "build:sdk": "cd sdk && npm run build",
    "build:server": "cd server && npm run build",
    "build": "npm run build:core && npm run build:sdk && npm run build:server",
    "test": "npm run build && cd secret && npm run compile && npx hardhat test",
    "deploy:goerli": "cd secret && hardhat deploy --network goerli"
  }
}
